              <script>
              function selecionar(){
                var menu = document.getElementById("sidebar");
                var espacio1 = document.getElementById("espacio1");
                var espacio2 = document.getElementById("espacio2");
                if(menu.classList.contains('fixed-top') == true){
                menu.classList.remove("fixed-top");
                espacio1.classList.add("mb-5");
                espacio2.classList.add("mb-4");
              }else{
                menu.classList.add("fixed-top");
                espacio1.classList.remove("mb-5");
                espacio2.classList.remove("mb-4");
              }
            }    
              function validar(){
                const monto = document.getElementById("monto_debito");
                "<% resurt.forEach((row)=> { %>";  
                let balance = <%= row.retirable %>;
                  if(balance < monto.value){
                    Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'No se puede retirar esta cantidad',
                    footer: '<span class="text-danger">Retirables Insuficientes!</span>',
                   }),
                    monto.value = "";
                  }
            "<%  }); %>"
             }
                function opciones() {
                  let radio = document.querySelector(
                    'input[name="search_radio"]:checked'
                  ).value;
                  var credito = document.querySelectorAll(".credito");
                  var egreso = document.querySelectorAll(".egreso");
                  var i;
                  if (radio == 3) {
                    for (i = 0; i < credito.length; i++) {
                      credito[i].classList.remove("d-none");
                      egreso[i].classList.remove("d-none");
                    }
                  } else if (radio == 2) {
                    for (i = 0; i <
                      credito.length; i++) {
                        credito[i].classList.add("d-none");
                      egreso[i].classList.remove("d-none");
                    }
                  } else if (radio == 1) {
                    for (i = 0; i <
                      egreso.length; i++) {
                        egreso[i].classList.add("d-none");
                      credito[i].classList.remove("d-none");
                    }
                  }
                } function retirable() {
                  const
                  ahorro = document.getElementById("monto_1").value; let = numero_1 = 0.7; let = numero_2 = 0.3;
                  num = ahorro * numero_2; num_1 = ahorro * numero_1;
                  document.getElementById("monto_2").value = num;
                  document.getElementById("monto_1").value = num_1;
                } function radio_retirable() {
                  let
                  retirable = document.getElementById("tipo"); if (retirable.value == 4) {
                    document.getElementById("monto_1").classList.add("d-none"); //
                    document.getElementById("monto_2").disabled = false;
                    document.getElementById("monto_1").required = false;
                  } else if (retirable.value == 5) {
                    document.getElementById("monto_1").classList.add("d-none"); //
                    document.getElementById("monto_2").disabled = false;
                    document.getElementById("monto_2").value = 500.00;
                    document.getElementById("monto_1").required = false;
                  } else if (retirable.value == 1 ||
                    retirable.value == 2 || retirable.value == 3) {
                    document.getElementById("monto_1").classList.remove("d-none"); //
                    document.getElementById("monto_1").required = true;
                  }
                } function Numeros(string) {
                  var
                  out = ""; var filtro = "1234567890."; for (var i = 0; i < string.length; i++) if
                    (filtro.indexOf(string.charAt(i)) != -1) out += string.charAt(i); return out;
                }
                 </script>
